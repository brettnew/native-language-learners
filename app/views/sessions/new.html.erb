<h1>Log In</h1>

<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %> <br>
    <%= text_field_tag :email, params[:email], :class => 'form-control' %>
  </div>
  <div class="field">
    <%= label_tag :password %> <br>
    <%= password_field_tag :password%><br><br>
    <%= submit_tag "Log In", :class => 'btn btn-primary' %>
  </div>
<% end %>
